{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":semanticCommitsDisabled"
  ],
  "enabledManagers": [
    "maven"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch"],
      "matchPackageNames": [
        "!com.h3xstream.findsecbugs:findsecbugs-plugin"
      ],
      "automerge": true
    }
  ]
}
